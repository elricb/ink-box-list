{
  "name": "@elricb/ink-box-list",
  "version": "0.0.3",
  "description": "List of wrapping boxes for Ink",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elricb/ink-box-list.git"
  },
  "author": "elricb (github.com/elricb)",
  "main": "lib",
  "engines": {
    "node": ">=11"
  },
  "scripts": {
    "build": "babel src/ -d lib/",
    "release": "npm run build && npm run lint && npm test && npm publish",
    "example": "node lib/example/index.js",
    "lint": "xo",
    "test": "ava",
    "test-update-snapshots": "ava --update-snapshots"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "ink",
    "component",
    "jsx",
    "react"
  ],
  "prettier": "@elricb/prettier-config",
  "ava": {
    "babel": {
      "testOptions": {
        "presets": [
          "@babel/preset-react"
        ]
      }
    }
  },
  "babel": {
    "presets": [],
    "plugins": [
      "@babel/plugin-transform-react-jsx"
    ]
  },
  "bugs": {
    "url": "https://github.com/elricb/ink-box-list/issues"
  },
  "homepage": "https://github.com/elricb/ink-box-list#readme",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "devDependencies": {
    "@ava/babel": "^2.0",
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@elricb/prettier-config": "0.0.1",
    "@elricb/xo-config": "^0.0.4",
    "ava": "^4.0.1",
    "eslint-config-xo-react": "^0.26.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "ink": "^3.2.0",
    "ink-testing-library": "^2.0.0",
    "ink-use-stdout-dimensions": "^1.0.5",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "react": "^17.0.2",
    "xo": "^0.47.0"
  },
  "peerDependencies": {
    "ink": ">=2",
    "prop-types": ">=14",
    "react": ">=16"
  }
}
